<h1>Task's details</h1>

<p><%= @task.title %></p>
<p><%= @task.details %></p>
<p><%= @task.completed %></p>

<%= link_to "Delete",
            task_path(@task),
            method: :delete,
            data: { confirm: "Are you sure?" } %>

<%= link_to 'Main', tasks_path %>

<%= link_to 'Edit', edit_path(@task) %>
